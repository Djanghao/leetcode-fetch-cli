{
  "name": "leetcode-fetch-cli",
  "version": "1.2.0",
  "description": "CLI tool to download LeetCode problems with solutions, pack and sync via Google Drive",
  "main": "bin/leetcode-fetch.js",
  "bin": {
    "leetcode-fetch": "./bin/leetcode-fetch.js"
  },
  "scripts": {
    "login": "node src/auth.js login",
    "download": "node src/download.js",
    "export": "node bin/leetcode-fetch.js export",
    "pack": "node bin/leetcode-fetch.js pack",
    "sync": "node bin/leetcode-fetch.js sync",
    "status": "node src/auth.js status",
    "logout": "node src/auth.js logout"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "fs-extra": "^10.0.0"
  },
  "keywords": [
    "leetcode",
    "download",
    "cli",
    "leetcode-downloader",
    "leetcode-problems",
    "coding-interview",
    "algorithm",
    "backup",
    "sync",
    "google-drive",
    "export",
    "multi-dataset"
  ],
  "author": "Houston Zhang",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Djanghao/leetcode-fetch-cli.git"
  },
  "bugs": {
    "url": "https://github.com/Djanghao/leetcode-fetch-cli/issues"
  },
  "homepage": "https://github.com/Djanghao/leetcode-fetch-cli#readme",
  "engines": {
    "node": ">=14.0.0"
  }
}
